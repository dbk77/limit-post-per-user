{
    "name": "dbk/limitpostsperuser",
    "type": "phpbb-extension",
    "description": "Allows you to set a maximum number of posts a user is allowed to make in any given timeframe,by default it's 24 hours",
    "homepage": "http://www.google.de",
    "version": "1.0",
    "keywords": [
        "phpbb",
        "extension",
        "user",
        "posts"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Davorka",
            "email": "budiselic@vodafone.de",
            "homepage": "http://www.google.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Limit Posts Per User",
        "soft-require": {
            "phpbb/phpbb": "3.1"
        }
    }
}